<resources>
    <!-- Full screen aspect ratio -->
    <bool name="config_haveHigherAspectRatioScreen">true</bool>

    <!-- Gestures' key codes. If the gesture is not supported set it to 0. -->
    <integer name="config_doubleTapKeyCode">354</integer>

    <!-- Whether the device supports Smart Pixels -->
    <bool name="config_enableSmartPixels">true</bool>

    <!-- Show a custom view for FOD -->
    <bool name="config_needCustomFODView">true</bool>

    <!-- Use default display mode -->
    <integer name="config_defaultLiveDisplayMode">0</integer>

    <!-- Flag specifying whether pocked mode is available on device -->
    <bool name="config_pocketModeSupported">false</bool>

</resources>
